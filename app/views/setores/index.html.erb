<h1>Listing setores</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Endereco</th>
    <th>Bairro</th>
    <th>Cidade</th>
    <th>Estado</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @setores.each do |setor| %>
  <tr>
    <td><%= setor.nome %></td>
    <td><%= setor.endereco %></td>
    <td><%= setor.bairro %></td>
    <td><%= setor.cidade %></td>
    <td><%= setor.estado %></td>
    <td><%= link_to 'Show', setor %></td>
    <td><%= link_to 'Edit', edit_setor_path(setor) %></td>
    <td><%= link_to 'Destroy', setor, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setor', new_setor_path %>
